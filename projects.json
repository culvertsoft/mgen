{
	"__COMMENT__": " This file is used for the nightly build script to specify what files should be uploaded. ",

	"def_files": {
		"java-assembly": [ "%PROJECT-assembly-%VERSION.jar" ],
		"java": ["%PROJECT-%VERSION.jar", "%PROJECT-%VERSION-javadoc.jar", "%PROJECT-%VERSION-sources.jar"],
		"cpp": ["%PROJECT-test"]
	},

	"path_to_projects": "./",

	"projects": {
		"mgen-api": {
			"def_files": "java"
		},
		"mgen-compiler": {
			"def_files": [ "java", "java-assembly" ]
		},
		"mgen-cppgenerator": {
			"def_files": "java"
		},
		"mgen-cpplib": {
			"def_files": "cpp"
		},
		"mgen-javagenerator": {
			"def_files": "java"
		},
		"mgen-javalib": {
			"def_files": "java"
		},
		"mgen-javascriptgenerator": {
			"def_files": "java"
		},
		"mgen-visualdesigner": {
			"def_files": [ "java", "java-assembly" ]
		}
	}
}
